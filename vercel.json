{
  "rewrites": [
    {
      "source": "/san-pham/:id",
      "has": [
        {
          "type": "header",
          "key": "User-Agent",
          "value": ".*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|WhatsApp|Zalo|TelegramBot).*"
        }
      ],
      "destination": "/api/product-og?id=:id"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}